{
  "name": "leoshop",
  "version": "1.0.0",
  "description": "MERN shopping application",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "build": "npm run build --prefix frontend; node backend/server",
    "client": "npm build --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "import_data": "node backend/seeder",
    "destroy_data": "node backend/seeder -d"
  },
  "author": "Leo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
